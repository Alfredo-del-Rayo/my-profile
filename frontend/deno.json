{
    "tasks": {
        "dev": "deno run -A npm:vite & deno run server:start",
        "build": "deno run -A npm:vite build",
        "server:start": "deno run -A --watch --env-file ./api/main.ts",
        "server:pstart": "deno run -A --watch ./api/main.ts",
        "serve": "deno task build && deno task server:pstart"
    },
    "imports": {
        "@bcrypt/": "https://deno.land/x/bcrypt/",
        "@oak/oakcors": "https://deno.land/x/cors@v1.2.2/mod.ts",
        "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
        "@oak/oak": "jsr:@oak/oak@^17.1.6",
        "@std/assert": "jsr:@std/assert@^1.0.12",
        "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
        "@tajpouria/cors": "jsr:@tajpouria/cors@^1.2.1",
        "@types/react": "npm:@types/react@^19.1.2",
        "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.4.1",
        "react": "npm:react@^19.1.0",
        "react-dom": "npm:react-dom@^19.1.0",
        "react-router-dom": "npm:react-router-dom@^7.5.1",
        "vite": "npm:vite@^6.3.2",
        "@socketio": "https://deno.land/x/socket_io@0.2.1/mod.ts",
        "@/": "./src/"
    },
    "nodeModulesDir": "auto",
    "compilerOptions": {
        "strict": true,
        "types": [
            "react",
            "react-dom",
            "@types/react",
            "./src/types/global.d.ts"
        ],
        "lib": [
            "dom",
            "dom.iterable",
            "deno.ns"
        ],
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}